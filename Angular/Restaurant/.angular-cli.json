{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "restaurant"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/font-awesome/css/font-awesome.css",
        "../src/assets/css/tether.min.css",
        "../src/assets/css/bootstrap.min.css",
        "../src/assets/Vendors/sweetalert2/css/sweetalert2.min.css",
        "../src/assets/Vendors/animate/animate.min.css",
        "../src/assets/Vendors/select2/css/select2.min.css",
        "../src/assets/css/app.css",
        "styles.css"
      ],
      "scripts": [
        "../src/assets/js/jquery.min.js",
        "../src/assets/js/tether.min.js",
        "../src/assets/js/bootstrap.min.js",
        "../src/assets/js/jquery.mask.js",
        "../src/assets/Vendors/remarkable-bootstrap-notify/bootstrap-notify.min.js",
        "../src/assets/Vendors/sweetalert2/js/sweetalert2.min.js",
        "../src/assets/Vendors/select2/js/select2.min.js",
        "../src/assets/js/app.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {},
    "serve": {
      "proxyConfig": "./proxy.config.js"
    }
  }
}
